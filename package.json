{
    "name": "nextjs-experiment",
    "version": "1.0.0",
    "private": true,
    "browserslist": [
        "last 2 Chrome versions",
        "last 2 ChromeAndroid versions",
        "last 2 Firefox versions",
        "last 2 Edge versions",
        "Safari >= 15",
        "iOS >= 15"
    ],
    "scripts": {
        "dev": "next dev",
        "build": "npm run codegen && next build",
        "start": "next start",
        "codegen": "graphql-codegen --config src/graphql/codegen.yml",
        "check": "npm run prettier:check && npm run eslint:check && npm run ts:check && npm run type-coverage:check",
        "fix": "npm run eslint:fix && npm run prettier:fix",
        "prettier:check": "prettier --check \"{pages,src,test}/**/*.{ts,tsx,json,graphql}\"",
        "prettier:fix": "prettier --write \"{pages,src,test}/**/*.{ts,tsx,json,graphql}\"",
        "eslint:check": "eslint --max-warnings=0 --report-unused-disable-directives --cache --cache-location target/.eslintcache pages src",
        "eslint:fix": "eslint --fix --cache --cache-location target/.eslintcache pages src",
        "ts:check": "tsc --noEmit",
        "type-coverage:check": "type-coverage",
        "favicon": "real-favicon generate src/assets/faviconDescription.json target/favicon.json public/favicon"
    },
    "typeCoverage": {
        "atLeast": 100,
        "detail": true
    },
    "dependencies": {
        "@apollo/client": "3.4.16",
        "@emotion/cache": "11.5.0",
        "@emotion/react": "11.5.0",
        "@emotion/server": "11.4.0",
        "@emotion/styled": "11.3.0",
        "@graphql-tools/merge": "8.2.1",
        "@graphql-tools/schema": "6.2.1",
        "@mui/icons-material": "5.0.5",
        "@mui/material": "5.0.6",
        "apollo-server-micro": "3.4.0",
        "cookie": "0.4.1",
        "fast-glob": "3.2.7",
        "graphql": "15.7.2",
        "jsonwebtoken": "8.5.1",
        "micro": "9.3.4",
        "ms": "2.1.3",
        "next": "12.0.1",
        "pg": "8.7.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "sequelize": "6.8.0",
        "sqlite3": "5.0.2"
    },
    "devDependencies": {
        "@graphql-codegen/add": "3.1.0",
        "@graphql-codegen/cli": "2.2.1",
        "@graphql-codegen/typescript": "2.2.4",
        "@graphql-codegen/typescript-operations": "2.1.8",
        "@graphql-codegen/typescript-resolvers": "2.3.2",
        "@types/cookie": "0.4.1",
        "@types/jsonwebtoken": "8.5.5",
        "@types/ms": "0.7.31",
        "@types/node": "14.14.31",
        "@types/react": "17.0.33",
        "@types/react-dom": "17.0.10",
        "@typescript-eslint/eslint-plugin": "5.2.0",
        "@typescript-eslint/parser": "5.2.0",
        "cli-real-favicon": "0.0.8",
        "eslint": "8.1.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-graphql": "4.0.0",
        "eslint-plugin-import": "2.25.2",
        "eslint-plugin-react": "7.26.1",
        "eslint-plugin-react-hooks": "4.2.0",
        "prettier": "2.4.1",
        "ts-node": "10.4.0",
        "type-coverage": "2.18.3",
        "typescript": "4.4.4",
        "vercel": "23.1.2",
        "webpack-bundle-analyzer": "4.5.0"
    }
}
